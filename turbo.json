{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": ["SSR"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {},
    "format": {}
  }
}
